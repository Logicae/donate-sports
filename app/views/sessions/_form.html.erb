    <%= form_for(:user, url: login_path) do |f| %>
        <ul><strong><%= f.label :email %></strong></ul>
        <ul><%= f.text_field :email %></ul>
        <ul><strong><%= f.label :password%></strong></ul>
        <ul><%= f.password_field :password %></ul>
        <ul><%= f.submit "Submit" %></ul>
    <% end %> 